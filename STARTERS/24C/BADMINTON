#include <iostream>
using namespace std;

int main() {
        int t;
        cin>>t;
        while(t--){
            int p,a;
           cin>>p; 
           if(p%2==0){
               a=p/2;
               cout<<a+1;
               cout<<endl;
           }
           else{
               a=(p-1)/2;
               cout<<a+1;
               cout<<endl;
           }
        }
	return 0;
}
